{
    "manifest_version": 3,
    "name": "Chrome Browser Search",
    "version": "0.0.0",
    "action": {
        "default_popup": "src/entrypoints/popup/index.html"
    },
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/entrypoints/content/index.ts"]
        }
    ]
}
